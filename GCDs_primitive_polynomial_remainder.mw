<Group labelreference="L123" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GcdPrimitive := proc(poly1::list,poly2::list)
    local p, q, c, u, v, r;
    p := poly1; q:= poly2;
    if nops(poly1)&lt;nops(poly2) then
        Error(&quot;Degree of the first argument needs to be bigger than second argument&quot;);
    end if;
    c := igcd(ContPoly(p),ContPoly(q));
    u := PrimePartPoly(p); v := PrimePartPoly(q);
    while nops(v) &gt; 1 do
