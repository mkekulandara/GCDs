<Group labelreference="L92" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">PrimePartPoly := proc(poly::list)
    local cont, i,u;
    cont := ContPoly(poly);
    u := [];
    if cont = 0 then
        u := poly;
    else
        for i from 1 by 1 to nops(poly) do
            u := [op(u), iquo(op(i,poly),cont)];
        end do;
    end if;
    return (u);
end proc:</Text-field>
</Input>
</Group>
