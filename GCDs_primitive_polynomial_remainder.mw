<Group labelreference="L121" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">DivPolynomial := proc(poly1::list,poly2::list)
    local q, r, u, v, m, n, k, i, j;
    u := poly1;
    m := nops(u);
    v := poly2;
    n := nops(v);
    r := u;
    q := [];
    for k from m-n+1 by -1 to 1 do
        q := [ op(q), 0 ];
    end do;
    for i from 1 by 1 to nops(q) do
        q[i] := iquo(r[i],v[1]);
        for j from 1 by 1 to n do
            r[i+j-1] := r[i+j-1] - q[i]*v[j];
        end do;
    end do;
    while r[1] = 0 and nops(r) &lt;&gt; 1 do
        r := subsop(1=NULL, r);
    end do;
    return([q,r]);
end proc:</Text-field>
</Input>
</Group>
