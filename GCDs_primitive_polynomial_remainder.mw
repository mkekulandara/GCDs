<Group labelreference="L117" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MultPolynomial := proc(poly1::list,poly2::list)
    local w, u, v, m, n, k, i, j;
    u := poly1;
    m := nops(u);
    v := poly2;
    n := nops(v);
    w := [];
    for k from n+m by -1 to 1 do
        w := [ op(w), 0 ];
    end do;
    for i from 1 by 1 to m do
        for j from 1 by 1 to n do
            w[i+j] := w[i+j] + u[i]*v[j];
        end do;
    end do;
    while w[1] = 0 and nops(w) &lt;&gt; 1 do
        w := subsop(1=NULL, w);
    end do;
    return (w);
end proc:</Text-field>
</Input>
</Group>
