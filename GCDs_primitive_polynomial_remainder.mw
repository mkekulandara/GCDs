<Group labelreference="L95" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">PsedudoDiv := proc(poly1::list,poly2::list)
    local dlta, vn, s,v,u,q;
    vn := op(1,poly2);
    dlta := nops(poly1) - nops(poly2);
    s := vn ** (dlta + 1);
    u := MultPolynomial(poly1,[s]);
    v := poly2;
    q := DivPolynomial(u,v);
    return([op(q),s]);
end proc:</Text-field>
</Input>
</Group>
